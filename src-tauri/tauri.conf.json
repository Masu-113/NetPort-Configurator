{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-app",
  "version": "0.1.0",
  "identifier": "com.tauriapp",
  "build": {
    "frontendDist": "../src",
    "beforeBuildCommand": "node src/modificacion.cjs"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Ideay-app",
        "width": 800,
        "height": 600,
        "url": "views/index.html",
        "resizable": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}