{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "NetPort-app",
  "version": "0.1.0",
  "identifier": "com.NetPort",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "NetPort-app",
        "width": 800,
        "height": 600,
        "url": "views/index.html",
        "resizable": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "../src-ps1/get_network_info.ps1",
      "../src-ps1/change_conf_port.ps1",
      "../src-ps1/change_conf_dhcp.ps1"
    ],
    "icon": [
      "icons/Squeare30x30Logo.png",
      "icons/Squeare44x44Logo.png",
      "icons/Squeare71x71Logo.png",
      "icons/Squeare89x89Logo.png",
      "icons/Squeare107x107Logo.png",
      "icons/Squeare142x142Logo.png",
      "icons/Squeare150x150Logo.png",
      "icons/Squeare284x284Logo.png",
      "icons/Squeare310x310Logo.png",
      "icons/icon.icns",
      "icons/ethernet.ico"     
    ]
  }
}
