{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "NetPort-app",
  "version": "0.1.1",
  "identifier": "com.NetPort",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "NetPort-app",
        "width": 800,
        "height": 600,
        "url": "views/index.html",
        "resizable": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/iconoNetPort.ico"     
    ]
  }
}
